{
  "kind": "collectionType",
  "collectionName": "blood_banks",
  "info": {
    "singularName": "blood-bank",
    "pluralName": "blood-banks",
    "displayName": "Blood Bank",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "blocks"
    },
    "phone": {
      "type": "biginteger"
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    },
    "donations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::donation.donation",
      "mappedBy": "blood_bank"
    },
    "blood_inventories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blood-inventory.blood-inventory",
      "mappedBy": "blood_bank"
    },
    "city": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::city.city",
      "inversedBy": "blood_banks"
    },
    "state": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::state.state",
      "inversedBy": "blood_banks"
    }
  }
}
